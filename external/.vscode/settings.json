// Place your settings in this file to overwrite default and user settings.
{
  // Configure glob patterns for excluding files and folders in the file explorer.
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/bower_components": true,
    "**/coverage": true,
    "**/lib-amd": true,
    "src/**/*.scss.ts": true
  },
  "typescript.tsdk": ".\\node_modules\\typescript\\lib",
  "sp-typed-item.config": [
    {
      "siteUrl": "https://uat-bhconnect.honedev.com/sites/preplan/devsite",
      "outputPath": "./src/GeneratedInterfaces",
      "lists": [
        {
          "url": "Lists/ERL",
          "fileName": "IERListItem",
          "fields": {
            "exclue": [
              "Title",
              "FieldInternalName"
            ],
            "excludeHidden": true
          }
        },
        {
          "url": "Lists/EBOM",
          "fileName": "IEBOMListItem",
          "fields": {
            "exclue": [
              "Title",
              "FieldInternalName"
            ],
            "excludeHidden": true
          }
        },
        {
          "url": "Lists/ECL",
          "fileName": "IECListItem",
          "fields": {
            "exclue": [
              "Title",
              "FieldInternalName"
            ],
            "excludeHidden": true
          }
        }
      ]
    }
  ]
}